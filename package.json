  "scripts": {
    "build": "gulp build",
    "test": "npm run build && node --require source-map-support/register node_modules/mocha/bin/mocha dist/**/*.test.js --reporter spec",
    "docs": "typedoc --mode file --out docs src --exclude \"**/*+(index|.spec|.test|.e2e).ts\"",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "npm-publish": "npm publish",
    "lint": "eslint \"./**/*.ts\"",
    "tools:testrig": "npm run build && node dist/tools/TestRig.js",
    "tools:testrig:html": "npm run tools:testrig -- read-test-case --file tools/local/local-debug.html --doc --format --token"
  },

  "scripts": {
    "heroku-postbuild": "BUILD_STANDALONE=true npm run build:production",
    "heroku-cleanup": "rimraf .jdk/ && rimraf node_modules/ && du -h -d 1",
    "playground": "node dist/server.js",
    "build:production": "rimraf dist/ && webpack --mode=production",
    "build:development": "rimraf dist/ && webpack --mode=development",
    "test": "npm run build:development && node --require source-map-support/register node_modules/mocha/bin/mocha dist/test/index.js --parallel --reporter spec --timeout 10000",
    "npm-publish": "npm run build:production && cd dist/ && npm publish && echo 'Do not forget to tag with current version and push the tag'",
    "lint": "eslint \"./**/*.ts\"",
    "testrig": "npm run build:development && node --require source-map-support/register dist/tools/TestRig.js",
    "testrig:html": "npm run testrig -- read-test-case --file tools/local/local-debug.html --doc --format --token",
    "testrig:print-doc": "npm run testrig -- print-doc"
  }
